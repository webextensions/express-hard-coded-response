{
  "name": "express-hard-coded-response",
  "version": "0.1.1",
  "description": "Express middleware to return hard-coded response for requests with url matching specified pattern",
  "main": "index.js",
  "scripts": {
    "jsdoc": "./node_modules/.bin/jsdoc index.js --destination jsdoc",
    "jsdoc2md": "jsdoc2md index.js > README.md",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "mocha": "./node_modules/.bin/mocha",
    "mocha-debug": "./node_modules/.bin/mocha --inspect --debug-brk",
    "pre-push": "npm run test && npm run jsdoc2md && git status",
    "test": "npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webextensions/express-hard-coded-response.git"
  },
  "keywords": [
    "express",
    "middleware",
    "hard-code",
    "response",
    "request",
    "url",
    "match",
    "pattern"
  ],
  "author": "Priyank Parashar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webextensions/express-hard-coded-response/issues"
  },
  "homepage": "https://github.com/webextensions/express-hard-coded-response#readme",
  "devDependencies": {
    "eslint": "3.16.1",
    "jsdoc": "3.4.3",
    "mocha": "3.2.0",
    "node-mocks-http": "1.6.1"
  },
  "dependencies": {
    "cjson": "0.5.0",
    "express-match-request": "0.1.1"
  }
}
